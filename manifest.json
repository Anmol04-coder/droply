{
  "manifest_version": 2,
  "name": "p5 sketch",
  "version": "0.1",
  "content_scripts": [{
      "matches": [
        "<all_urls>"
      ],
      "js": ["p5.js", "script.js"]
    }
  ],
  "browser_action": {
      "default_icon": "droply.pgn",
      "default_popup": "popup.html",
      "default_title": "Droply"
  }
}
